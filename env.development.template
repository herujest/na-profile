# Development Environment Variables
# Copy this file to .env.development and fill in your values
# Command: cp env.development.template .env.development

# Database Configuration (Development)
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/nisa_profile_dev?connection_limit=10&pool_timeout=20

# Node Environment
NODE_ENV=development

# Next.js Configuration
NEXT_TELEMETRY_DISABLED=1

# Admin Authentication
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_dev_password_here

# R2/S3 Configuration (Development)
R2_ENDPOINT=https://your-r2-endpoint.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=your_r2_access_key_id
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key
R2_BUCKET_NAME=your-bucket-name-dev
R2_PUBLIC_URL=https://your-public-url-dev.com
NEXT_PUBLIC_R2_PUBLIC_BASE=https://your-public-url-dev.com

# PostgreSQL Configuration (for docker-compose)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=nisa_profile_dev
POSTGRES_PORT=5433

# Application Port
APP_PORT=3001

