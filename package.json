    "build:prod": "./scripts/build-prod.sh",
    "start": "next start",
    "start:dev": "NODE_ENV=development next start -p 3001",
    "start:prod": "NODE_ENV=production next start -p 5175",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "migrate:deploy": "prisma migrate deploy",
