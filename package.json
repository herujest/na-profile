    "db:prod:down": "docker-compose -f docker-compose.prod.yml --env-file .env.production down",
    "db:prod:logs": "docker-compose -f docker-compose.prod.yml --env-file .env.production logs -f",
    "db:prod:migrate": "./scripts/migrate-prod.sh deploy",
    "db:prod:studio": "./scripts/studio-prod.sh",
    "db:prod:build": "docker-compose -f docker-compose.prod.yml --env-file .env.production up -d --build",
    "docker:build": "./scripts/docker-build-prod.sh --local",
    "docker:build:local": "./scripts/docker-build-prod.sh --local",
    "docker:build:push": "./scripts/docker-build-prod.sh --push",
    "docker:build:push:amd": "./scripts/docker-build-prod.sh --push-amd",
    "docker:build:dev": "./scripts/docker-build-dev.sh --local",
    "docker:build:dev:local": "./scripts/docker-build-dev.sh --local",
    "docker:build:dev:push": "./scripts/docker-build-dev.sh --push",
    "docker:build:dev:push:amd": "./scripts/docker-build-dev.sh --push-amd",
    "docker:build:prod": "./scripts/docker-build-prod.sh --local",
    "docker:build:prod:local": "./scripts/docker-build-prod.sh --local",
    "docker:build:prod:push": "./scripts/docker-build-prod.sh --push",
    "docker:build:prod:push:amd": "./scripts/docker-build-prod.sh --push-amd"
