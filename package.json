    "db:studio": "prisma studio",
    "db:dev:up": "docker-compose -f docker-compose.dev.yml --env-file .env.development up -d",
    "db:dev:down": "docker-compose -f docker-compose.dev.yml --env-file .env.development down",
    "db:dev:logs": "docker-compose -f docker-compose.dev.yml --env-file .env.development logs -f",
